{
  "version": 2,
  "builds": [
    {
      "src": "api/webhook.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/health.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/stats.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/webhook",
      "dest": "/api/webhook.ts"
    },
    {
      "src": "/api/health",
      "dest": "/api/health.ts"
    },
    {
      "src": "/api/stats",
      "dest": "/api/stats.ts"
    }
  ],
  "functions": {
    "api/*.ts": {
      "runtime": "@vercel/node@3"
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}
